
@{
    ViewBag.Title = "Cars";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




<div class="container">
    <div class="page-header">
        <h1>Car Rentals</h1>
    </div>

    <div class="row">

        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>Booking ID</th>
                    <th>Car ID</th>

                    <th>Cost per Day (£)</th>

                </tr>
            </thead>

            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@Html.Encode(item.Id)</td>
                        <td>@Html.Encode(item.CarID)</td>

                        <td>@Html.Encode(item.Cost)</td>

                    </tr>
                }
            </tbody>
        </table>

    </div>


</div>